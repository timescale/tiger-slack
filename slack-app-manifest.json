{
  "display_information": {
    "name": "tiger-slack-ingest",
    "description": "This bot ingests slack events into a Timescaledb database",
    "background_color": "#000000"
  },
  "features": {
    "bot_user": {
      "display_name": "tiger-slack-ingest",
      "always_online": true
    }
  },
  "oauth_config": {
    "scopes": {
      "user": [
        "channels:history"
      ],
      "bot": [
        "channels:history",
        "channels:read",
        "users.profile:read",
        "users:read",
        "users:read.email",
        "reactions:read"
      ]
    }
  },
  "settings": {
    "event_subscriptions": {
      "user_events": [
        "message.channels"
      ],
      "bot_events": [
        "channel_created",
        "channel_rename",
        "message.channels",
        "reaction_added",
        "reaction_removed",
        "team_join",
        "user_change",
        "user_profile_changed"
      ]
    },
    "interactivity": {
      "is_enabled": true
    },
    "org_deploy_enabled": false,
    "socket_mode_enabled": true,
    "token_rotation_enabled": false
  }
}